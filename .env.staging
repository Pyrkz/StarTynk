# Database
DATABASE_URL="postgresql://startynk_staging:staging_password@staging-db.startynk.com:5432/startynk_staging"
DATABASE_URL_DIRECT="postgresql://startynk_staging:staging_password@staging-db.startynk.com:5432/startynk_staging"

# Authentication
JWT_SECRET="staging-jwt-secret-use-env-vars-in-production"
JWT_REFRESH_SECRET="staging-refresh-secret-use-env-vars-in-production"
NEXTAUTH_SECRET="staging-nextauth-secret-use-env-vars-in-production"
NEXTAUTH_URL="https://staging.startynk.com"

# API Configuration
API_VERSION="v1"
API_RATE_LIMIT_WINDOW="900000"
API_RATE_LIMIT_MAX="100"

# Mobile App Configuration
EXPO_PUBLIC_API_URL="https://staging-api.startynk.com/api/v1"
EXPO_PUBLIC_APP_NAME="StarTynk Staging"
EXPO_PUBLIC_APP_SCHEME="startynk-staging"
EXPO_PUBLIC_ENVIRONMENT="staging"

# Web App Configuration
NEXT_PUBLIC_API_URL="https://staging-api.startynk.com/api/v1"
NEXT_PUBLIC_APP_NAME="StarTynk Staging"
NEXT_PUBLIC_ENVIRONMENT="staging"

# External Services (Optional)
SENTRY_DSN="https://your-sentry-dsn@sentry.io/staging"
SENTRY_AUTH_TOKEN=""
SENTRY_ORG=""
SENTRY_PROJECT=""

# Push Notifications (Optional)
EXPO_PUBLIC_PUSH_TOKEN=""
FCM_SERVER_KEY=""

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=""
MIXPANEL_TOKEN=""

# AWS (Optional)
AWS_REGION="eu-central-1"
AWS_ACCESS_KEY_ID=""
AWS_SECRET_ACCESS_KEY=""
