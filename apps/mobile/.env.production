# Production Environment Configuration
# Live production settings

# =================
# ENVIRONMENT TYPE
# =================
EXPO_PUBLIC_ENVIRONMENT=production

# =================
# API CONFIGURATION
# =================
# Production server URL - Update with your actual production URL
EXPO_PUBLIC_API_URL=https://api.startynk.com/api/v1

# =================
# APP CONFIGURATION
# =================
EXPO_PUBLIC_APP_NAME=StarTynk
EXPO_PUBLIC_APP_SCHEME=startynk
EXPO_PUBLIC_APP_VERSION=1.0.0

# =================
# OPTIONAL SERVICES
# =================
# Production Sentry DSN for error tracking
# EXPO_PUBLIC_SENTRY_DSN=https://your-production-sentry-dsn@sentry.io/project-id

# Production WebSocket URL
# EXPO_PUBLIC_WEBSOCKET_URL=wss://api.startynk.com

# =================
# FEATURE FLAGS
# =================
# Production environment feature flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_LOGS=false
EXPO_PUBLIC_MOCK_API=false