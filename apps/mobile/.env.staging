# Staging Environment Configuration
# Testing/staging environment settings

# =================
# ENVIRONMENT TYPE
# =================
EXPO_PUBLIC_ENVIRONMENT=staging

# =================
# API CONFIGURATION
# =================
# Staging server URL - Update with your actual staging URL
EXPO_PUBLIC_API_URL=https://staging-api.startynk.com/api/v1

# =================
# APP CONFIGURATION
# =================
EXPO_PUBLIC_APP_NAME=StarTynk Staging
EXPO_PUBLIC_APP_SCHEME=startynk-staging
EXPO_PUBLIC_APP_VERSION=1.0.0

# =================
# OPTIONAL SERVICES
# =================
# Staging Sentry DSN for error tracking
# EXPO_PUBLIC_SENTRY_DSN=https://your-staging-sentry-dsn@sentry.io/project-id

# Staging WebSocket URL
# EXPO_PUBLIC_WEBSOCKET_URL=wss://staging-api.startynk.com

# =================
# FEATURE FLAGS
# =================
# Staging environment feature flags (enable monitoring for testing)
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_LOGS=true
EXPO_PUBLIC_MOCK_API=false